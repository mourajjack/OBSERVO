<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage
    x:Class="OBSERVO.Views.MainFlyoutPage"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:pages="clr-namespace:OBSERVO.Views"
    FlyoutLayoutBehavior="Popover">

    <FlyoutPage.Flyout>
        <pages:Menu />
    </FlyoutPage.Flyout>

    <FlyoutPage.Detail>
        <NavigationPage>
            <x:Arguments>
                <pages:MainTabbedPage />
            </x:Arguments>
        </NavigationPage>
    </FlyoutPage.Detail>

</FlyoutPage>